{
  "name": "musiclackey",
  "version": "0.0.1",
  "description": "Music Releases Bot",
  "author": "Laqwan Brodie",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "pup": "ts-node src/services/facebooklogin"
  },
  "dependencies": {
    "@apollo/gateway": "^0.42.3",
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/graphql": "^9.0.6",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^9.0.0",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/serve-static": "^1.0.1",
    "@types/lodash": "^4.14.144",
    "@types/newrelic": "^5.11.1",
    "@types/node-schedule": "^1.2.4",
    "@types/oauth": "^0.9.1",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-facebook-token": "^0.4.33",
    "@types/puppeteer": "^5.4.4",
    "@types/validator": "^13.6.3",
    "apify": "^0.17.0",
    "apollo-server-express": "^3.4.0",
    "concurrently": "^5.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.6.1",
    "graphql-tools": "^8.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "nest-winston": "^1.1.2",
    "newrelic": "^8.5.0",
    "node-schedule": "^1.3.2",
    "oauth": "^0.9.15",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^3.3.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^2.0.0",
    "puppeteer-extra": "^3.1.7",
    "query-params": "0.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "spotify-web-api-node": "^4.0.0",
    "ts-morph": "^12.0.0",
    "ts-node": "^8.4.1",
    "type-graphql": "^1.1.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^6.7.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^27.0.2",
    "@types/mongoose": "^5.5.21",
    "@types/node": "^12.7.5",
    "@types/passport-local": "^1.0.33",
    "@types/spotify-web-api-node": "^4.0.1",
    "@types/supertest": "^2.0.8",
    "jest": "^27.3.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "tsc-watch": "^4.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
